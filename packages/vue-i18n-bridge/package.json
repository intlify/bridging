{
  "name": "@intlify/vue-i18n-bridge",
  "version": "0.3.6",
  "description": "Vue I18n bridging for Vue 2 & Vue 3",
  "scripts": {
    "postinstall": "node ./scripts/postinstall.js"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.0.0-rc.1",
    "vue-i18n": "^8.26.1 || ^9.2.0-beta.25"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "main": "lib/index.cjs",
  "module": "lib/index.mjs",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "require": "./lib/index.cjs",
      "import": "./lib/index.mjs"
    },
    "./*": "./*"
  },
  "keywords": [
    "vue",
    "vuejs",
    "vue-i18n",
    "i18n",
    "internationalization",
    "intlify",
    "bridge",
    "bridging",
    "stub"
  ],
  "files": [
    "lib",
    "bin",
    "scripts",
    "LICENSE",
    "README.md"
  ],
  "bin": {
    "vue-i18n-fix": "bin/fix.js",
    "vue-i18n-switch": "bin/switch.js"
  },
  "license": "MIT",
  "author": {
    "name": "kazuya kawaguchi",
    "email": "kawakazu80@gmail.com"
  },
  "homepage": "https://github.com/intlify/bridging/tree/main/packages/vue-i18n-bridge#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/intlify/bridging.git",
    "directory": "packages/vue-i18n-bridge"
  },
  "bugs": {
    "url": "https://github.com/intlify/bridging/issues"
  },
  "engines": {
    "node": ">= 12"
  },
  "publishConfig": {
    "access": "public"
  }
}
